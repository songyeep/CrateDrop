<ol class="breadcrumb">
  <li><a href="/">Home</a></li>
  <li><a href="/categories">Categories</a></li>
  <li><a href="/categories/<%= @category.id %>"><%= @category.name %></a></li>
</ol>

<p id="notice"><%= notice %></p>

<div class="container">
  <% @crates.each do |crate| %>
    <div class="crate-list"><a href="/crates/<%= crate.id%>"><%= crate.name %></a></p>
    <p id="crate-temp"><%= image_tag(crate.image) %></p></div>
  <% end %>
</div>